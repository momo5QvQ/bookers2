<%= render 'devise/shared/user_info', user: @user_info %>
<%= render 'devise/shared/new_book', book: @book %>

<h2>Users</h2><!--user一覧-->
<table>
  <thead>
    <tr>
      <th>image</th>
      <th>name</th>
    </tr>
  </thead>
  <% @users.each do |user| %>
  <tbody>
    <tr>
      <td><%= image_tag user.get_profile_image(100,100) %></td>
      <td><%= user.name %></td>
      <td><%= link_to 'show', user_path(user) %></td>
    </tr>
  </tbody>
  <% end %>
</table>